{
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/picsTransform.js",
    "\\.(css|sass|scss)$": "<rootDir>/src/tests/styleMock.js"
  },
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/src/tests/setupTests.js"]
}
